{
    "name": "bogordesaincom/laravel-enum-simple",
    "description": "Elegant Enum implementation for Laravel. With make:enum command, ability to casting for Eloquent models and labels translations via lang files. Forked from ",
    "type": "library",
    "keywords": [
        "bogordesaincom",
        "enum",
        "laravel-enum"
    ],
    "homepage": "https://github.com/bogordesaincom/laravel-enum-simple",
    "license": "MIT",
    "authors": [
        {
            "name": "bogordesaincom",
            "email": "nakamura.agatha@gmail.com",
            "homepage": "https://github.com/bogordesaincom",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0 || ^8.1 || ^8.2 || ^8.3",
        "illuminate/console": "^7.0 || ^8.0 || ^9.0 || ^9.1 || ^10 || ^11",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^9.1 || ^10 || ^11",
        "illuminate/translation": "^7.0 || ^8.0 || ^9.0 || ^9.1 || ^10 || ^11",
        "myclabs/php-enum": "^1.8.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.7",
        "orchestra/testbench": "^5.0 || ^6.0 || ^7.0 || ^8.0",
        "phpunit/phpunit": "^8.0 || ^9.3 || ^10 || ^11"
    },
    "autoload": {
        "psr-4": {
            "Bogordesain\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bogordesain\\Enum\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bogordesain\\Enum\\EnumServiceProvider"
            ]
        }
    }
}
